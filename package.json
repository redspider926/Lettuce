{
  "name": "lettus",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "adb-nox": "adb connect localhost:62025",
    "adb-mobile": "adb tcpip 5555",
    "adb-reverse": "adb reverse tcp:8081 tcp:8081",
    "android-build-debug": "cd android/ && ./gradlew assembleDebug",
    "android-build-release": "cd android && ./gradlew assembleRelease",
    "android-clean": "cd android && ./gradlew clean",
    "android-run-debug": "react-native run-android",
    "android-run-release": "react-native run-android --variant=release",
    "android-getKey": "keytool -list -v -keystore ./android/app/release.keystore -alias lettuce -storepass lettuce -keypass lettuce",
    "android-signkey": "keytool -genkey -v -keystore release.keystore -alias lettuce -keyalg RSA -keysize 2048 -validity 10000",
    "android-signingReport": "cd android/ && ./gradlew signingReport",
    "reset-cache": "react-native start --reset-cache"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
